<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>com.whxiaoyu</groupId>
		<artifactId>whxiaoyu-components</artifactId>
		<version>${revision}</version>
	</parent>

	<artifactId>whxiaoyu-component-oss-starter</artifactId>
	<name>whxiaoyu-component-oss-starter</name>
	<description>对象存储服务</description>
	<packaging>jar</packaging>

	<properties>
		<obs.version>3.21.8</obs.version>
		<minio.version>8.2.1</minio.version>
		<oss.version>3.8.1</oss.version>
	</properties>

	<dependencies>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-autoconfigure</artifactId>
			<optional>true</optional>
		</dependency>

		<dependency>
			<groupId>io.minio</groupId>
			<artifactId>minio</artifactId>
			<version>${minio.version}</version>
			<optional>true</optional>
		</dependency>

		<dependency>
			<groupId>com.huaweicloud</groupId>
			<artifactId>esdk-obs-java</artifactId>
			<version>${obs.version}</version>
			<optional>true</optional>
		</dependency>

		<dependency>
			<groupId>com.aliyun.oss</groupId>
			<artifactId>aliyun-sdk-oss</artifactId>
			<version>${oss.version}</version>
			<optional>true</optional>
		</dependency>


	</dependencies>



</project>
